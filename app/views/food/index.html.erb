<table class="table-list">
<tr>
  <th>Food</th>
  <th>Measurement Unit</th>
  <th>Unit Price</th>
  <th>Quantity</th>
  <th>Actions</th>
</tr>
<% @food.each do |food| %>
  <tr>
    <td><%= food.name %></td>
    <td><%= food.measurement_unit %></td>
    <td><%= food.price %></td>
    <td><%= food.quantity %></td>
    <td></td>
  </tr>
<% end %>
</table>